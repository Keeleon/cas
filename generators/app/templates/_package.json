{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "start": "node dist/index.js",
    "compile": "tsc && tsc -p test/tsconfig.json",
    "lint": "tslint -p tsconfig.json && tslint -p test/tsconfig.json",
    "unit-test": "mocha",
    "watch-unit-test": "mocha --recursive --compilers ts:ts-node/register --watch test/**/*.spec.*",
    "pretest": "npm run compile && npm run lint",
    "test": "npm run unit-test",
    "posttest": "npm run mutate && npm run cover",
    "mutate": "stryker run",
    "cover": "nyc npm run unit-test"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "dependencies": {
    "channelape-logger": "^0.3.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/html-reporter": "^2.1.0",
    "@stryker-mutator/mocha-framework": "^2.1.0",
    "@stryker-mutator/mocha-runner": "^2.1.0",
    "@stryker-mutator/typescript": "^2.1.0",
    "@types/sinon-express-mock": "^1.3.7",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.6",
    "@types/sinon": "^7.5.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "sinon-express-mock": "^2.2.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.6.4"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "./reports/lcov"
  },
  "mocha": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extension": [
      "ts"
    ],
    "spec": [
      "test/**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}